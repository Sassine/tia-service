<?xml version="1.0" encoding="UTF-8"?>
<Configuration>

	<Loggers>
		<Root level="info">
			<AppenderRef ref="Console" />
		</Root>
	</Loggers>

	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d{ISO8601} %p [%t] %C{1.}: %m%n%throwable" />
		</Console>
	</Appenders>
</Configuration>